{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",

    "logLevel": {
      "default": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Trace",
      "Function.DownloadTrigger": "Debug"
    },

    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}
